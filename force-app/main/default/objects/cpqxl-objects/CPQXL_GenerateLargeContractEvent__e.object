<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Event used to manage generating a very large contract.</description>
    <eventType>HighVolume</eventType>
    <fields>
        <fullName>CPQXL_MainContractId__c</fullName>
        <description>Stores the id of the main (large) contract generated by the Generate Very Large Contract functionality.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Main Contract Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CPQXL_NumberOfEventsAccumulated__c</fullName>
        <description>Since errors encountered in the CPQ managed &quot;;Generate Contract&quot; queueable thread cannot be properly caught, this field stores the number of events accumulated in the Generate Large Contract platform event chain. If it reaches high enough, we will stop adding additional events to the chain.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Number of Events Accumulated</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CPQXL_OrderId__c</fullName>
        <description>Stores the Id of an Order (subset Order) to contract.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Order Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CPQXL_PrimaryQuoteId__c</fullName>
        <description>Id of the Primary Quote involved in a Generate Large Contract process.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Primary Quote Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CPQXL_RemainingSubsetOrderIds__c</fullName>
        <description>Stores the Ids of Orders we have yet to generate Contract/Subscriptions for.</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Remaining Subset Order Ids</label>
        <length>10000</length>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>CPQXL_SourceContext__c</fullName>
        <description>Stores whether contracting from the Order, or contracting from the Opportunity. Note: contracting from the Opportunity is not supported (yet).</description>
        <externalId>false</externalId>
        <isFilteringDisabled>false</isFilteringDisabled>
        <isNameField>false</isNameField>
        <isSortingDisabled>false</isSortingDisabled>
        <label>Source Context</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>CPQXL Generate Large Contract Event</label>
    <pluralLabel>CPQXL Generate Large Contract Events</pluralLabel>
    <publishBehavior>PublishAfterCommit</publishBehavior>
</CustomObject>
